{
  "name": "personal-website",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "now": {
    "alias": "francesco-agnoletto.com",
    "version": 1,
    "env": {
      "DB_USER": "@db_user"
    }
  },
  "scripts": {
    "start": "node server.js",
    "start--dev": "cross-env NODE_ENV=development webpack -w & node server.js",
    "build": "cross-env NODE_ENV=production webpack -p",
    "test": "jest --coverage --verbose",
    "test--ci": "jest --coverage --runInBand --forceExit",
    "test--watch": "jest --watch --verbose",
    "test--snapshots": "jest --updateSnapshot",
    "type-check": "tsc --noEmit",
    "tslint": "tslint --fix --project ./",
    "lint": "stylelint 'src/client/**/*.ts' 'src/client/**/*.tsx'",
    "prettier": "prettier --parser typescript --write 'src/client/**/*.ts' 'src/client/**/*.tsx'",
    "migrate-stories": "babel-node --presets @babel/env src/server/scripts/migration-script.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
